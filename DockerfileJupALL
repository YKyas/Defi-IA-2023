FROM python
RUN git clone https://github.com/YKyas/Defi-IA-2023.git
WORKDIR Defi-IA-2023
RUN pip install -r requirements.txt
WORKDIR models
RUN wget https://drive.google.com/uc?id=1JKSCVvMCDbVyLiamoe1J77pMq5XPq8YJ -O regressor_xgb.joblib
WORKDIR  ..
EXPOSE 8888
CMD ["jupyter","notebook","--ip=0.0.0.0","--port=8888","--no-browser","--allow-root","--NotebookApp.token=DefiIa" ]